{
  "name": "metawrite",
  "license": "BSD-3-Clause",
  "version": "0.1.9",
  "description": "Appwrite SDK with ready to go components for Svelte / SvelteKit",
  "keywords": [
    "svelte",
    "sveltekit",
    "appwrite",
    "sdk",
    "components",
    "library"
  ],
  "repository": "github:koodeau/metawrite",
  "homepage": "https://increas.io/metawrite",
  "bugs": {
    "url": "https://github.com/koodeau/metawrite/issues",
    "email": "koodeau@null.net"
  },
  "author": {
    "name": "Nick Revenko",
    "email": "koodeau@null.net",
    "url": "https://koodeau.me/"
  },
  "logo": {
    "file": "./static/logo.svg"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "prettier": "prettier --write --plugin-search-dir= ./src/lib/**/*.{js,svelte} && prettier --write --plugin-search-dir= ./src/lib/*.{js,svelte}",
    "svelte-check": "svelte-check",
    "version": "auto-changelog -p --hide-credit --template changelog.hbs && git add CHANGELOG.md",
    "test": "svelte-check --workspace src/lib",
    "release:beta": "npm run package && cd package && npm publish --tag beta",
    "release": "npm run package && cd package && npm publish"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "auto-changelog": "^2.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.4.3",
    "svelte-preprocess": "^4.10.1",
    "svelte2tsx": "^0.5.3",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "files": [
    "**/*.svelte",
    "**/*.js",
    "**/*.d.ts",
    "*.svelte",
    "*.js",
    "*.d.ts"
  ],
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "dependencies": {
    "appwrite": "^6.0.1"
  }
}
